exec_repeat:
; Need to push HL to the operator stack, this points to the expression
; need to push the repeat token to the operator stack
; need to push a 0 to the operator stack
; skip to the end of the line
; execute as normal

  ex de,hl
  ld hl,(opstack_ptr)
  dec hl
  ld (hl),d
  dec hl
  ld (hl),e
  dec hl
  ld (hl),$D2
  dec hl
  ld (hl),$00
  ld (opstack_ptr),hl
  ex de,hl
  jr seek_EOL



