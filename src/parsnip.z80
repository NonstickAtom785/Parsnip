pop_operand:
; Need to pop off the operand stack
; leave with DE pointing to the start of the element data, A as the type-1
  ld de,(operandstack_ptr)
  dec de
  ld a,(de)
  cp type_var
  jr z,popvar_to
  ld c,a
  dec c
  ld hl,type_size_LUT
  add a,l
  ld l,a
  jr nc,$+3
  inc h
  ld a,(hl)
  or a
  jr z,pop_to_variable_size
  ;need to do (DE-A+1))
  dec a
  ld b,a
  ld a,e
  sub b
  ld e,a
  jr nc,$+3
  dec d
  ld (operandstack_ptr),de
  ld a,c
  ret

