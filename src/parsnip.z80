peekoperand_2:
;A is the type of the first
;C is the type of the second
  ld hl,(operandstack_ptr)
  dec hl
  ld c,(hl)
  ld a,c
  cp type_var
  call z,peekvar
  ld de,type_size_LUT
  add a,e
  ld e,a
  jr nc,$+3
  inc d
  ld a,(de)
  or a
  jr z,variable_size
  ld e,a
  ld a,l
  sub e
  ld l,a
  jr nc,$+3
  dec h

  ld a,(hl)
  cp type_var
  jp z,get_var_type   ;destroys HL
  ret

peekvar:
;preserve HL,A
  push hl
  call get_var_type
  ld c,a
  pop hl
  ld a,type_var
  ret

variable_size:
;For now, these have size bytes, might change in the future?
